{
  "entryPoints": ["src/index.js", "src/app.js", "src/commit/generator.js", "src/git/operations.js", "src/api/client.js", "src/config/index.js", "src/errors/index.js", "src/security/sanitizer.js", "src/di/container.js"],
  "out": "docs",
  "name": "ai-commit",
  "readme": "README.md",
  "plugin": ["typedoc-plugin-markdown"],
