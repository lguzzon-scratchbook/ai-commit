  "categoryOrder": ["*", "Internal"],
  "sort": ["source-order"],
  "visibility": "public",
  "defaultCategory": "General",
  "categoryTitleMappings": {
    "General": "API Reference"
  },
  "toc": [
    {
      "text": "Getting Started",
      "items": [
        "installation",
        "usage"
      ]
